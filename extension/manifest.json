{
    "name": "Epoch Buddy",
    "short_name": "Epoch Buddy",
    "description": "Convert Epoch to Human Readable Time with some extra features",
    "version": "1.1.0",
    "manifest_version": 3,
    "homepage_url": "https://github.com/ShivamS136/epoch-buddy",
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "64": "icons/icon64.png",
        "128": "icons/icon128.png",
        "256": "icons/icon256.png",
        "512": "icons/icon512.png"
    },
    "action": {
        "default_title": "Epoch Buddy",
        "default_popup": "index.html",
        "default_icon": "logo.png"
    },
    "permissions": [
        "storage"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "script.js"
            ],
            "css": [
                "script.css"
            ]
        }
    ]
}
