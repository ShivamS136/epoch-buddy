{
    "name": "Epoch Buddy",
    "short_name": "Epoch Buddy",
    "description": "Convert Epoch to Human Readable Time with some extra features",
    "version": "1.0.1.0",
    "manifest_version": 3,
    "homepage_url": "https://github.com/ShivamS136/epoch-buddy",
    "action": {
        "default_title": "Epoch Buddy",
        "default_popup": "index.html",
        "default_icon": "logo.png"
    },
    "permissions": [
        "storage"
    ],
    "offline_enabled": true,
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "script.js"
            ],
            "css": [
                "script.css"
            ]
        }
    ]
}